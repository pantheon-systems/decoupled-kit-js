{
  "name": "decoupled-kit-js",
  "version": "0.0.0",
  "description": "Pantheon's Decoupled JavaScript starter kits and SDKs",
  "main": "index.js",
  "scripts": {
    "test": "pnpm recursive test",
    "build:pkgs": "pnpm --filter ./packages run build",
    "build:starters": "pnpm --filter ./starters run build",
    "build:all": "pnpm recursive run build",
    "generate-docs": "pnpm --filter ./web clear && pnpm --filter ./web build",
    "postinstall": "pnpm build:pkgs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pantheon-systems/decoupled-kit-js.git"
  },
  "keywords": [
    "pantheon",
    "pantheon-systems",
    "decoupled",
    "decoupled-kit",
    "headless-cms-sdk"
  ],
  "author": "@pantheon-systems",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pantheon-systems/decoupled-kit-js/issues"
  },
  "homepage": "https://github.com/pantheon-systems/decoupled-kit-js#readme",
  "devDependencies": {
    "@algolia/client-search": "^4.9.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/react": "17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "rollup": "^2.69.1",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.12",
    "typescript": "4.5.5",
    "vite": "^2.8.6"
  },
  "packageManager": "pnpm@6.32.3",
  "pnpm": {
    "overrides": {
      "@types/react": "17.0.40",
      "node-fetch@<2.6.7": ">=2.6.7",
      "ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
      "nth-check@<2.0.1": ">=2.0.1",
