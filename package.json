    "jest-junit": "^13.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "rollup": "^2.69.1",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.12",
    "typescript": "4.5.5",
    "vite": "3.0.0"
  },
  "packageManager": "pnpm@7.4.0",
  "pnpm": {
    "overrides": {
      "@types/react": "17.0.40",
      "node-fetch@<2.6.7": ">=2.6.7",
      "ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
      "nth-check@<2.0.1": ">=2.0.1",
      "immer@<9.0.6": ">=9.0.6",
      "ansi-html@<=0.0.7": "<=0.0.9",
