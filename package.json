      "@pantheon-systems/drupal-kit": "workspace:*",
      "moment@<2.29.2": ">=2.29.2",
      "async@<3.2.2": ">=3.2.2",
      "cross-fetch@<3.1.5": ">=3.1.5",
      "sharp@<0.30.5": ">=0.30.5",
      "devcert@<1.2.1": ">=1.2.1",
      "jpeg-js@<0.4.4": ">=0.4.4",
      "got@<11.8.5": ">=11.8.5",
