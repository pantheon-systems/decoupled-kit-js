{
  "name": "decoupled-kit-js",
  "private": true,
  "version": "1.0.0",
  "description": "Pantheon Decoupled Kit Monorepo",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/pantheon-systems/decoupled-kit-js#readme",
