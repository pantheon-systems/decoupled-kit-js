{{#if drupal}}
.content {
	display: flex;
	flex-direction: column;
	margin: var(--12) auto;
	max-width: var(--md);
}

.content > ul h2 {
	font-size: var(--7);
	font-weight: 700;
	margin-bottom: var(--8);
}

.link {
	text-decoration-line: underline;
}

.notFound {
	text-align: center;
	margin: var(--14) auto 0;
	font-size: var(--7);
	font-weight: 600;
}
{{/if}}
