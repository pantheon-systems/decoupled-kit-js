import { INormalizedModules, ITemplate } from "codesandbox-import-util-types";
export declare function getMainFile(template: ITemplate): "server.js" | "src/main.js" | "src/main.ts" | "src/main.tsx" | "index.html" | "src/pages/index.js" | "src/pages/Index.vue" | "deck.mdx" | "package.json" | "src/index.js";
declare type Dependencies = {
    [name: string]: string;
};
declare type PackageJSON = {
    dependencies?: Dependencies;
    devDependencies?: Dependencies;
};
export declare function getTemplate(pkg: PackageJSON | null, modules: INormalizedModules): ITemplate | undefined;
