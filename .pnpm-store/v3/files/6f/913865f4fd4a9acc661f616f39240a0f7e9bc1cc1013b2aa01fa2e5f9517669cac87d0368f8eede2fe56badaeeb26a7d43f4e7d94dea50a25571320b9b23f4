import type { ImageCropFocus, WidthOrHeight } from "../types";
export declare function generatePublicUrl({ url, mimeType, }: {
    url: string;
    mimeType: string;
}, checkMimeType?: boolean): string;
export declare function generateImageArgs({ width, height, format, cropFocus, quality, }: WidthOrHeight & {
    format: string;
    cropFocus?: ImageCropFocus | Array<ImageCropFocus>;
    quality: number;
}): string;
