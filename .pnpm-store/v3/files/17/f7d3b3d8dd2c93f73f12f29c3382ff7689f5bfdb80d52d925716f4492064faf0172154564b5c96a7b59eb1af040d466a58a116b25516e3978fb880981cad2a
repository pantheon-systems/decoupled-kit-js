   __schema {\n      types {\n        kind\n        name\n        description\n\n        possibleTypes {\n          kind\n          name\n        }\n        interfaces {\n          kind\n          name\n        }\n        enumValues {\n          name\n        }\n        ofType {\n          kind\n          name\n          ofType {\n            kind\n            name\n            ofType {\n              kind\n              name\n            }\n          }\n        }\n        fields {\n          name\n          description\n          args {\n            name\n            type {\n              kind\n              name\n              inputFields {\n                name\n              }\n            }\n          }\n          type {\n            name\n            kind\n            ofType {\n              kind\n              name\n              ofType {\n                kind\n                name\n                ofType {\n                  kind\n                  name\n                }\n              }\n            }\n          }\n        }\n      }\n\n      mutationType {\n        fields {\n          type {\n            name\n          }\n        }\n      }\n    }\n  }\n`\n"],"file":"graphql-queries.js"}