import { Location } from 'src/types';
export declare function setLocation(location: Location): {
    '@type': string;
    address: {
        streetAddress: string;
        addressLocality: string;
        addressRegion?: string | undefined;
        postalCode: string;
        addressCountry: string;
        '@type': string;
    } | undefined;
    name: string;
    sameAs?: string | undefined;
} | undefined;
