import { MutableRefObject } from "react";
import { GatsbyImageProps } from "./gatsby-image.browser";
declare type LazyHydrateProps = Omit<GatsbyImageProps, "as" | "style" | "className"> & {
    isLoading: boolean;
    isLoaded: boolean;
    toggleIsLoaded: (toggle: boolean) => void;
    ref: MutableRefObject<HTMLImageElement | undefined>;
};
export declare function lazyHydrate({ image, loading, isLoading, isLoaded, toggleIsLoaded, ref, imgClassName, imgStyle, objectPosition, backgroundColor, objectFit, ...props }: LazyHydrateProps, root: MutableRefObject<HTMLElement | undefined>, hydrated: MutableRefObject<boolean>, forceHydrate: MutableRefObject<boolean>): (() => void) | null;
export {};
