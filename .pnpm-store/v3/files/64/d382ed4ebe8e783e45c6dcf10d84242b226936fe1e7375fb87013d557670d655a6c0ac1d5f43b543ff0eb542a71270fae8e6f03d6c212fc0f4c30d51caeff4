import type { Actions } from "gatsby";
import type { ImageFit } from "../types";
export declare function shouldDispatch(): boolean;
export declare function dispatchLocalFileServiceJob({ url, filename, mimeType, contentDigest, }: {
    url: string;
    filename: string;
    mimeType: string;
    contentDigest: string;
}, actions: Actions): void;
export declare function dispatchLocalImageServiceJob({ url, extension, basename, width, height, format, fit, contentDigest, quality, }: {
    url: string;
    extension: string;
    basename: string;
    width: number;
    height: number;
    format: string;
    fit: ImageFit;
    contentDigest: string;
    quality: number;
}, actions: Actions): void;
